{
  "extends": "airbnb",
  "plugins": ["compat"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "compat/compat": 2,
    "react/destructuring-assignment": 0,
    "react/jsx-props-no-spreading": 0
  }
}
